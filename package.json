{
  "name": "social_app_server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run migrate:db & npm run seed:db",
    "migrate:db": "sequelize-cli db:migrate",
    "seed:db": "sequelize-cli db:seed:all",
    "start": "npm run start:app & npm run start:cron & npm run start:notification & npm run start:socket",
    "start:app": "node src/index.js",
    "start:cron": "node src/workers/cron-jobs/index.js",
    "start:notification": "node src/workers/notification-jobs/index.js",
    "start:socket": "node src/workers/socket-server/index.js",
    "develop": "npm run develop:app & npm run develop:cron & npm run develop:notification & npm run develop:socket",
    "develop:app": "env DEBUG=api* nodemon --inspect src/index.js",
    "develop:cron": "env DEBUG=api* nodemon --inspect=1234 src/workers/cron-jobs/index.js",
    "develop:notification": "env DEBUG=api* nodemon --inspect=1235 src/workers/notification-jobs/index.js",
    "develop:socket": "env DEBUG=api* nodemon --inspect=1236 src/workers/socket-server/index.js"
  },
  "author": "Renganatha A<renganatha10@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.12.1",
    "bull-arena": "^2.6.4",
    "concurrently": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.1",
    "socket.io": "^2.5.0",
    "toobusy-js": "^0.5.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.1"
  }
