{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["import", "prettier"],
  "globals": {
    "describe": true,
    "it": true,
    "expect": true,
    "__dirname": false,
    "alert": false,
    "Blob": false,
    "clearImmediate": true,
    "clearInterval": false,
    "clearTimeout": false,
    "console": false,
    "escape": false,
    "Event": false,
    "EventTarget": false,
    "exports": false,
    "fetch": false,
    "File": false,
    "FileReader": false,
    "FormData": false,
    "global": true,
    "Map": true,
    "module": false,
    "navigator": false,
    "process": false,
    "Promise": true,
    "requestAnimationFrame": true,
    "requestIdleCallback": true,
    "require": false,
    "Set": true,
    "setImmediate": true,
    "setInterval": false,
    "setTimeout": false,
    "WebSocket": false,
    "window": false,
    "XMLHttpRequest": false,
    "jest": true
  },
  "rules": {
    "indent": "off",
    "no-var": 2,
    "prefer-destructuring": 2,
    "prefer-spread": 2,
    "no-console": 2,
    "no-useless-constructor": 2,
    "require-atomic-updates": 0,
    "no-async-promise-executor": 0,
    "import/order": 2,
    "import/newline-after-import": 2,
    "prettier/prettier": 2
  }
}
